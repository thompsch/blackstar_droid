        app:layout_constraintStart_toEndOf="@+id/box_sharp_1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/box_sharp_3"
        android:layout_width="27dp"
        android:layout_height="66dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/empty_rectangle"
        app:layout_constraintBottom_toBottomOf="@+id/box_sharp_2"
        app:layout_constraintStart_toEndOf="@+id/box_sharp_2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/box_sharp_4"
        android:layout_width="27dp"
        android:layout_height="56dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/empty_rectangle"
        app:layout_constraintBottom_toBottomOf="@+id/box_sharp_3"
        app:layout_constraintStart_toEndOf="@+id/box_sharp_3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

